{
  "name": "react-flux",
  "version": "0.0.1",
  "description": "react flux",
  "private": true,
  "scripts": {
    "build": "browserify src/client/index.js -t babelify | uglifyjs > public/bundle.js",
    "watch": "watchify -d src/client/index.js -t babelify -o public/bundle.js -v",
    "browser": "browser-sync start --server public --files public/**/*",
    "server": "node index.js"
  },
  "dependencies": {
    "babel": "^5.0.12",
    "express": "^4.12.3",
    "immutable": "^3.7.1",
    "object-assign": "^2.0.0",
    "react": "^0.13.1",
    "socket.io": "^1.3.5"
  },
  "devDependencies": {
    "babelify": "^6.0.2",
    "browser-sync": "^2.5.0",
    "browserify": "^9.0.3",
    "uglify-js": "^2.4.19",
    "watchify": "^3.1.0"
  }
}
